{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": ["test/@types", "src/@types", "node_modules/@types"],
    "types": [
      "vue-apollo",
      "testcafe",
      "jest",
      "quasar"
    ]
  },
  "exclude": [
    "/dist",
    ".quasar",
    "node_modules"
  ]
}