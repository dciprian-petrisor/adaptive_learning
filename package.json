{
  "name": "adaptive_learning",
  "version": "0.0.1",
  "description": "Adaptive Learning Application",
  "productName": "Adaptive Learning",
  "author": "Petrisor Ciprian <dciprian.petrisor@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "make dev",
    "build": "quasar build",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-server-and-test \"quasar dev\" http://localhost:8080 \"testcafe chromium:headless test/testcafe/__tests__\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-server-and-test \"quasar dev\" http://localhost:8080 \"testcafe chromium:headless test/testcafe/__tests__\""
  },
